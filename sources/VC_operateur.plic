programme plicoperateur {

    ecrire 5000 > 1  ; //1
    ecrire 5 > 1000  ; //0
    ecrire 1 > 1  ; //0

    ecrire 5 < 1000  ; //1
    ecrire 5000 < 1  ; //0
    ecrire 1 < 1  ; //0


    ecrire 1 <= 1  ; //1
    ecrire 1 >= 1  ; //1

    ecrire 1 = 1  ; //1
    ecrire 1 # 2  ; //1
    ecrire 1 # 1  ; //0
    ecrire 1 = 2  ; //0

    ecrire 1 + 1 ;
    ecrire 4 - 1 ;
    ecrire 4 - 10 ;
    ecrire 2 * 4 ;

    ecrire ( 2 < 5 ) et ( ( 2 < 4 ) ou ( 45 > 50 ) ) ;

}