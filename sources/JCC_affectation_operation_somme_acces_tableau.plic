programme progPlic {
	entier i ; 
	entier j ;
	entier k ; 
	tableau [ 10 ] tab ;

	tab [ 0 ] := 8 ;
	tab [ 1 ] := 9 ;
	tab [ 2 ] := 1 ;
	tab [ 3 ] := tab [ tab [ 2 ] ] + tab [ 0 ] ; // 17
	tab [ tab [ 1 ] ] := tab [ 0 ] + 70 ; // 78
	tab [ tab [ 1 ] ] := tab [ tab [ 1 ] ] + 70 ; // 148
	tab [ tab [ 2 ] ] := tab [ tab [ 1 ] ] + tab [ tab [ 1 ] ] ; // 296

	i := tab [ 3 ] ;
	j := tab [ 9 ] ;
	k := tab [ 1 ] ;
